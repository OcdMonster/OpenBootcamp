//En el archivo index.js crea una función que devuelva un error con un mensaje personalizado

const miFuncion = val => {
    if(typeof val === "number") {
        console.log(2*val)
        
    }
    throw new Error("El valor debe ser tipo numero");
}
const Double = miFuncion("Perro");

